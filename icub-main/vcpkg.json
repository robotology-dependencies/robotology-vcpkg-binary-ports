{
  "name": "icub-main",
  "version": "2.4.1",
  "homepage": "https://github.com/robotology/icub-main",
  "description": "The iCub Main Software Repository",
  "license": "BSD-3-Clause",
  "dependencies": [
    "ace",
    "yarp",
    {
      "name": "ycm-cmake-modules",
